[Plugin]
Name=lcdctrl_sram_to_lcd
Description=Adapts lcdctrl_sram to generic lcd
SourceDir=platform\fpga\drivers\lcd\adaptors\lcdctrl_sram\src
Implement=lcd_adaptor
InitHandler=lcdctrl_sram_to_lcd_init
InitHandlerPriority=16
CompileOption0=-I"$(SSASROOT)\platform\fpga\drivers\lcd\internal"

[Instance]
Require0=per_lcdctrl_sram

