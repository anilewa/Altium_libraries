[Plugin]
Name=gps_nmea_to_serial
Description=Posix interface for GPS_NMEA serial port.
SourceDir=services\posix\devio\serial\adaptors\gps_nmea\src
CompileOption0=-I"$(SSASROOT)\platform\fpga\drivers\uart8\internal"
CompileOption1=-I"$(SSASROOT)\platform\fpga\drivers\gps_nmea\internal"
CompileOption2=-I"$(SSASROOT)\services\posix\devio\serial\include"
InitHandler=gps_nmea_to_serial_init
InitHandlerPriority=8
Implement=serial_adaptor

[Instance]
Require0=drv_gps_nmea
